{
  "name": "@squonk/sdf-parser",
  "private": false,
  "version": "1.3.0",
  "contributors": [
    {
      "name": "Informatics Matters Ltd"
    },
    {
      "name": "Symeres",
      "url": "https://github.com/Symeres/sdf-parser"
    }
  ],
  "description": "A parser for SDF files with streaming support",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "tsc": "tsc --pretty --noEmit",
    "lint": "eslint --cache --max-warnings=0",
    "format": "eslint --fix --cache --max-warnings=0",
    "test": "vitest"
  },
  "devDependencies": {
    "@squonk/eslint-config": "2.0.0",
    "@types/node": "22.9.0",
    "eslint": "8.57.0",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "vitest": "1.5.0",
    "node-fetch": "3.3.2"
  }
}
